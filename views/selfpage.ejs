<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">    
<html manifest="demo.appcache"> 
<head>    
	<title>Music For Web</title>   
	    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
		<meta http-equiv="content-type" content="text/html; charset=utf-8"/>
		<meta name="format-detection" content="telephone=no"/>
		<link rel='stylesheet' href="/stylesheets/public_style.css" />
		<script type="text/javascript" language="javascript" src="/javascripts/jquery-2.0.3.js"></script>
        <script type="text/javascript" language="javascript" src="/javascripts/jquery-2.0.3.min.js"></script>
		<script type="text/javascript" language="javascript" src="/javascripts/public_javascript.js"></script>
</head>    
	<body> 
		<div id="viewlayer" class="viewlayer">
        	<div id="menu_box" class="menu_box">
           	  <img src="/stylesheets/images/title.png" class="title_img" />
                <form>
                	<input type="text" class="search_input"id="search_input">
                    <input type="button" class="search_button" value="search" onclick="search()">
                </form>
				<button class="logout">LOGOUT</button>
               <button class="self">SELF</button>
            </div><!--menu box-->
            <div class="content">
				<div id="playBox" class="playBox box-shadow">
					<div class="criticism">
						<img class="userPic" src="<%=%>">
						<input type="text" class="sName" value="<%=%>" readonly="readonly">
						<input type="button" class="changPass" value="change password" onclick="showForm()">
						<form id="changePassword" style="display:none;">
							<input type="text" class="cInput" placeholder=" please enter old password" id="oldPass">
							<input type="text" class="cInput" placeholder=" please enter new password" id="newPass">
							<input type="button" class="changPass" value="submit" onclick="ChangePass()">
						</form>
					</div>
					<div class="detials">
						<div class="title_word">
						Self List
							<div style="float:right;">
								<input type="button" class="s_button2" value="Last" onclick="">
								<input type="button" class="s_button2" value="1" disabled="disabled">
								<input type="button" class="s_button2" value="Next" onclick="">
							</div>
						</div>
						<table class="table_04">
							<tr>
								<td class="tdLine">
									<input type="button" class="sSongInfo" value="<%=%>" id="<%=%>" onclick="">
								</td>
								<td class="tdLine"><input type="checkbox" class="" id=""></td>
							</tr>
						</table>
					</div>
				</div>
            </div>
			<div class="footer">
			</div>
		</div><!--viewlayer-->
	</body>    
</html>